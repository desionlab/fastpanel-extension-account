{
  "name": "fastpanel-extension-account",
  "version": "0.0.1",
  "description": "Add-on to work with users of the system.",
  "author": "Desionlab <fenixphp@gmail.com>",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "scripts": {
    "develop": "./node_modules/.bin/tsc --watch",
    "build": "./node_modules/.bin/tsc && npm run test && npm run docs",
    "docs": "typedoc --theme minimal --target 'esnext' --excludeNotExported --excludePrivate --ignoreCompilerErrors --out ./docs/ ./src/",
    "test": "node ./node_modules/mocha/bin/mocha --require ts-node/register  test/**/*.spec.ts",
    "prepare": "./node_modules/.bin/tsc && npm run test && npm run docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/desionlab/fastpanel-extension-account.git"
  },
  "bugs": {
    "url": "https://github.com/desionlab/fastpanel-extension-account/issues"
  },
  "homepage": "https://github.com/desionlab/fastpanel-extension-account#readme",
  "keywords": [
    "nodejs",
    "typescript"
  ],
  "dependencies": {
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.6",
    "@types/mocha": "^5.2.5",
    "@types/mongoose": "^5.3.2",
    "@types/node": "^10.12.10",
    "@types/passport": "^0.4.7",
    "@types/passport-http-bearer": "^1.0.33",
    "@types/passport-local": "^1.0.33",
    "@types/socket.io": "^2.1.0",
    "@types/vorpal": "^1.11.0",
    "chai": "^4.2.0",
    "fastpanel-core": "github:desionlab/fastpanel-core#develop",
    "fastpanel-extension-httpx": "github:desionlab/fastpanel-extension-httpx#develop",
    "fastpanel-extension-mongodb": "github:desionlab/fastpanel-extension-mongodb#develop",
    "fastpanel-extension-socket.io": "github:desionlab/fastpanel-extension-socket.io#develop",
    "mocha": "^5.2.0",
    "ts-node": "^7.0.1",
    "typedoc": "^0.13.0",
    "typescript": "^3.2.1"
  },
  "license": "MIT"
}
